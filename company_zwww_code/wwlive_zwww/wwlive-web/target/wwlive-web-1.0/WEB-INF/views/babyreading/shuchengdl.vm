<script type="text/javascript">
    window.onload = function () {
        //根据参数值，获取路径中相应的参数值。
        function getUrlParam(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)"); //构造一个含有目标参数的正则表达式对象
            var r = window.location.search.substr(1).match(reg);  //匹配目标参数
            //alert(r);
            if (r != null) return unescape(r[2]); return null; //返回参数值
        }
        var vt;
        vt = getUrlParam("s");
        if (vt && vt=="sc"){
            $(".zb").show();
        }
        $(".wrap").on("click",function(){
            //进入页面判断是ios终端还是安卓终端，或者是浏览器
            var browser = BrowserInfo();
            if (browser.isIphone) {
                $(".toast").show();
                setTimeout(function () {
                    $(".toast").hide();
                },1500)
            }else if (browser.isAndroid){//安卓终端
                if (browser.isWeixin){
                    $(".r").show();
                }else {
                    if (vt && vt=="sc"){
                        window.location.href = "https://ks3-cn-beijing.ksyun.com/apkdown/apk_download/dl/dl-app-release_20008.apk";
                    }else {
                        window.location.href = "https://ks3-cn-beijing.ksyun.com/apkdown/apk_download/dl/dl-app-release_20009.apk";
                    }
                }

            }
        })
        $(".download").on("click",function(){
            //进入页面判断是ios终端还是安卓终端，或者是浏览器
            var browser = BrowserInfo();
            if (browser.isIphone) {
                $(".toast").show();
                setTimeout(function () {
                    $(".toast").hide();
                },1500)
            }else if (browser.isAndroid){//安卓终端
                if (browser.isWeixin){
                    $(".r").show();
                }else {
                    if (vt && vt=="sc"){
                        window.location.href = "https://ks3-cn-beijing.ksyun.com/apkdown/apk_download/dl/dl-app-release_20008.apk";
                    }else {
                        window.location.href = "https://ks3-cn-beijing.ksyun.com/apkdown/apk_download/dl/dl-app-release_20009.apk";
                    }
                }

            }
        })
        function BrowserInfo () {
            var json = {
                userAgent: navigator.userAgent.toLowerCase(),
                isAndroid: Boolean(navigator.userAgent.match(/android/ig)),
                isIphone: Boolean(navigator.userAgent.match(/iphone|ipod/ig)),
                isIpad: Boolean(navigator.userAgent.match(/ipad/ig)),
                isWeixin: Boolean(navigator.userAgent.match(/MicroMessenger/ig)),
            }
            return json;
        }

    }
</script>

